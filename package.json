{
  "name": "my-sample-app",
  "version": "0.0.1",
  "description": "Sample serverless React prototype app making use of AWS Cognito, Lambda and DynamoDB",
  "repository": {
    "type": "git",
    "url": "https://github.com/seuvitor/react-aws-cognito-lambda-dynamodb-sample-prototype-app.git"
  },
  "scripts": {
    "lint": "eslint --fix --ignore-pattern '/dist/**' .",
    "start": "webpack-dev-server --content-base ./dist --port 5000 --entry ./src/my-sample-app.js --output-filename my-sample-app.min.js --devtool source-map --env development --mode development",
    "build": "webpack --entry ./src/my-sample-app.js --output-filename my-sample-app.min.js --env production --mode production"
  },
